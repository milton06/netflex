{% extends 'base.html.twig' %}

{% block body %}
    <h1>Price</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ price.id }}</td>
            </tr>
            <tr>
                <th>Orderbasecharge</th>
                <td>{{ price.orderBaseCharge }}</td>
            </tr>
            <tr>
                <th>Orderextraweightleviedcharge</th>
                <td>{{ price.orderExtraWeightLeviedCharge }}</td>
            </tr>
            <tr>
                <th>Orderrevisedbasecharge</th>
                <td>{{ price.orderRevisedBaseCharge }}</td>
            </tr>
            <tr>
                <th>Orderrevisedextraweightleviedcharge</th>
                <td>{{ price.orderRevisedExtraWeightLeviedCharge }}</td>
            </tr>
            <tr>
                <th>Orderinvoiceprice</th>
                <td>{{ price.orderInvoicePrice }}</td>
            </tr>
            <tr>
                <th>Ordercodpaymentaddedcharge</th>
                <td>{{ price.orderCodPaymentAddedCharge }}</td>
            </tr>
            <tr>
                <th>Orderfuelsurchargeaddedcharge</th>
                <td>{{ price.orderFuelSurchargeAddedCharge }}</td>
